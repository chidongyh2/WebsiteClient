{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            false,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            false,
            "element",
            "app",
            "kebab-case"
        ]
    },
    "angularCompilerOptions": { "fullTemplateTypeCheck": true, "strictInjectionParameters": true, "enableIvy": false }
}
